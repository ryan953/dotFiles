antigen use oh-my-zsh

antigen bundles <<EOBUNDLES
  colored-man-pages
  command-not-found
  safe-paste

  z # https://github.com/ohmyzsh/ohmyzsh/tree/master/plugins/z
  changyuheng/fz

  zsh-users/zsh-autosuggestions
  zsh-users/zsh-completions

  # Apply syntax highlighting to result of auto-suggest, completions, etc.
  zsh-users/zsh-syntax-highlighting

  # Must be after zsh-syntax-highlighting
  history-substring-search

  # Requires "export NVM_COMPLETION=true" be set earlier in .zshrc
  lukechilds/zsh-nvm # https://github.com/lukechilds/zsh-nvm

  lukechilds/zsh-better-npm-completion # https://github.com/lukechilds/zsh-better-npm-completion
EOBUNDLES

antigen theme romkatv/powerlevel10k

antigen apply
